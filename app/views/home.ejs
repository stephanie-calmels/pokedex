<%- include('./partials/header') %> 

    <% pokemons.forEach(pokemon => { %>
        <article class="card">
            <a href="/details/<%= pokemon.id %>">
                <img class="card__img" src="/img/<%= pokemon.numero %>.png " alt="photo de <%= pokemon.nom %> ">
                <h3 class="card__title">#<%= pokemon.numero %> <%= pokemon.nom %></h3>
            </a>
        </article>
    <% }) %>

<%- include('./partials/footer') %> 
